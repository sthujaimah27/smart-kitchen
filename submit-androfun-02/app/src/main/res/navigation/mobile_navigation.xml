<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_finished">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.androidmobile_sub02.ui.home.HomeFragment"
        android:label="Smart Kitchen"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_finished"
        android:name="com.example.androidmobile_sub02.ui.finishedEvent.FinishedEventFragment"
        android:label="Smart Kitchen"
        tools:layout="@layout/fragment_finished_event" />

    <fragment
        android:id="@+id/navigation_upcoming"
        android:name="com.example.androidmobile_sub02.ui.scanPage.ImageAnalysisFragment"
        android:label="Smart Kitchen"
        tools:layout="@layout/fragment_camera_features" />

</navigation>